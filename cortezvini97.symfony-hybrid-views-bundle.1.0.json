{
    "manifests": {
        "cortezvini97/symfony-hybrid-views-bundle": {
            "manifest": {
                "bundles": {
                    "Cortez\\SymfonyHybridViews\\SymfonyHybridViews": [
                        "all"
                    ]
                },
                "copy-from-package": {
                    "Resources/symfony_hybrid_views.yaml": "%CONFIG_DIR%/packages/symfony_hybrid_views.yaml"
                },
                "copy-from-recipe": {
                    "bin/something.sh": "%BIN_DIR%/something",
                    "bin/symfony_hybrid_views.yaml":"%BIN_DIR%/symfony_hybrid_views.yaml"
                },
                "post-install-output": [
                    "<bg=green;fg=white> Symfony Hybrid Views Installed </>"
                ]
            },
            "files": {
                "bin/symfony_hybrid_views.yaml":{
                    "contents":[
                        "symfony_hybrid_views:",
                        "    dir_views: 'views'",
                        "    cache_dir: 'blade_cache'",
                        "    directives_dir: 'blade_directives'",
                        "    functions_dir: 'blade_functions'",
                        "    cache: false"
                    ],
                    "executable":false
                },
                "bin/something.sh": {
                    "contents": [
                        "#!/usr/bin/env bash",
                        "",
                        "echo \"hello world\"",
                        ""
                    ],
                    "executable": true
                }
            },
            "ref": "099a603dc5a4e182a5a4d4b3c76511a21acb7c40"
        }
    }
}
